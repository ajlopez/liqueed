<div class="row actions">
<a class="btn btn-primary" href="<%= adminprefix %>/project">Projects</a>
<a class="btn btn-primary" href="<%= adminprefix %>/project/<%= projectid %>">Project</a>
<a class="btn btn-primary" href="<%= adminprefix %>/project/<%= projectid %>/decision/new">New Decision</a>
</div>
<div class="row">
<table class="table-striped table-bordered">
    <tr>
        <th>&nbsp;</th>
        <th>Description</th>
    </tr>
<% items.forEach(function (item) { %>
    <tr>
        <td><a href="<%= adminprefix %>/project/<%= projectid %>/decision/<%= item.id %>">View</a></td>
        <td><%= item.description %></td>
    </tr>
<% }); %>
</table>
</div>
