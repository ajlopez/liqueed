<div class="row actions">
<a class="btn btn-primary" href="<%= adminprefix %>/project/new">New Project</a>
</div>
<div class="row">
<table class="table-striped table-bordered">
    <tr>
        <th></th>
        <th>Name</th>
    </tr>
<% items.forEach(function (item) { %>
    <tr>
        <td><a href="<%= adminprefix %>/project/<%= item.id %>">View</a></td>
        <td><%= item.name %></td>
    </tr>
<% }); %>
</table>
</div>
